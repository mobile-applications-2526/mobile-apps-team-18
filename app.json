{
  "expo": {
    "name": "KotConnect",
    "slug": "KotConnect",
    "scheme": "kotconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "icon": "./assets/splash-icon2.png",
    "splash": {
      "image": "./assets/splash-icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#111827",
      "bundleIdentifier": "com.n4thanj.KotConnect",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon2.png",
        "backgroundColor": "#111827"
      },
      "package": "com.n4thanj.KotConnect"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon2.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ec7557b0-a884-48ab-873d-0272dc08c663"
      }
    },
    "owner": "n4thanj"
  }
}
